def main x do
    (a, b) = (1, 2);
    assert (a == 1);
    assert (b == 2);
    
    ((c, d), e) = ((3, 4), 5);
    assert (c == 3);
    assert (d == 4);
    assert (e == 5);

	[h, t] = [6, 7];
	assert (h == 6);
	assert (t == 7);

	assert (trycatch check_none failed == 'success');
end

def check_none x do
	[h, t] = [6, 7, 8];
end

def failed x do
	<- 'success';
end